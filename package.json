{
  "name": "napoli",
  "version": "1.0.0",
  "description": "test project for school ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "up:build": "npm run dev$npm_config_old -- up --build -d",
    "dev":"docker compose -f docker-compose.yml -f docker-compose.devcontainer.yml",
    "up": "npm run dev$npm_config_old -- up -d",
    "down": "npm run dev$npm_config_old -- down -v",
    "back:reset": "npm run reset:db && npm run back",
    "back":"npm run dev$npm_config_old -- exec back npm run start:debug",
    "front": "npm run dev$npm_config_old -- exec front npm run start",
    "reset:db":"sudo rm -rdf ./pgdata && npm run dev$npm_config_old -- restart db"
  },
